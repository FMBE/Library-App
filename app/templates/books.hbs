<h1>Books</h1>

<table class="table table-bordered">
  <thead>
  <tr>
    <th>
      Title
    </th>
    <th>Author</th>
    <th>Year</th>
  </tr>
  </thead>
  <tbody>
  {{#each model as |book|}}
    <tr>
      <td>
          <span>{{book.title}}</span>
      </td>
      <td>
        <span>{{book.author.name}}</span>
      </td>
      <td>
        <span>{{book.releaseYear}}</span>
      </td>
    </tr>
  {{/each}}
  </tbody>
</table>
